<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_selected="false">
        <shape android:shape="oval">
            <stroke android:width="@dimen/regular_dimen_x2" android:color="#ffbababa" />
            <size android:height="@dimen/regular_dimen_x9" android:width="@dimen/regular_dimen_x9" />
        </shape>
    </item>
    <item android:state_selected="true">
        <shape android:shape="oval">
            <stroke android:width="@dimen/regular_dimen_x2" android:color="#ff24c668" />
            <solid android:color="#ff24c668" />
            <size android:height="@dimen/regular_dimen_x9" android:width="@dimen/regular_dimen_x9" />
        </shape>
    </item>
</selector>